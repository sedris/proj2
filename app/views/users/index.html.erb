<h1>Registered Users</h1>
<table class="table">
  <tr>
    <th>Username</th>
    <th>Type</th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.type %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />