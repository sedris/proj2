<h1>Listing All Orders</h1>
<table>
  <tr>
    <th>ID</th>
    <th>Cart ID</th>
    <th>Cart Owner</th>
    <th>Shopkeepers</th>
    <th></th>
  </tr>

<% @orders.each do |order| %>
  <tr>
    <td><%= order.id %></td>
    <td><%= order.cart.id %></td>
    <td><%= order.cart.shopper_id %></td>
    <td><% order.shopkeepers.each do |shopkeeper|%><%=shopkeeper.email + " " %><%end%></td>
    <td></td>
  </tr>
<% end %>
</table>

<br />